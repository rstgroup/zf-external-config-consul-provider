{
    "name": "rstgroup/zf-external-config-consul-provider",
    "description": "Consul provider for rstgroup/zf-external-config-module",
    "type": "library",
    "authors": [
        {
            "name": "Artur Wolny",
            "email": "artur.wolny@rst.com.pl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 5000,
        "secure-http": false
    },
    "require": {
        "php": ">= 5.6",
        "rstgroup/zf-external-config-module": "dev-feature/mvp",
        "rstgroup/php-consul-config-provider": "dev-feature/mvp"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "RstGroup\\ZfExternalConfigConsulProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RstGroup\\ZfExternalConfigConsulProvider\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "/home/arturwolny/projects/platformers/php/zf-external-config-module"
        },
        {
            "type": "vcs",
            "url": "/home/arturwolny/projects/platformers/php/consul-config-provider"
        },
        {
            "type": "vcs",
            "url": "https://github.com/artospaj/consul-php-sdk"
        }
    ],
    "scripts": {
        "unit-tests": "phpunit",
        "unit-tests-coverage": "phpunit --coverage-html ./results"
    }
}
